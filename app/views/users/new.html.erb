<h1>New User Form</h1>

<%= form_for @user, url: {action: "create"} do |f| %>
  <%= f.label :username, "Enter Username" %>
  <%= f.text_field :username, placeholder: "username here" %>
  <%= f.label :email, "Enter email" %>
  <%= f.email_field :email, placeholder: "email here" %>
  <%= f.label :password, "Secret" %>
  <%= f.password_field :password, placeholder: "password here"  %>
  <%= f.submit "Create User" %>
<% end %>
